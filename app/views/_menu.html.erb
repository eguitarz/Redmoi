<% if current_user %>

<p><%= link_to 'NEW', new_gather_path %></p>
<p id="menu-edit"><%= link_to_function 'EDIT', 'test()' %></p>
<ul>
<% @gathers.each do |g| %>
  <li>
    <span class="menu-del"><%= link_to 'x', gather_path(g), :method => :delete %></span>
    <span class="gather"><%= link_to g.title, gather_path(g) %></span>
  </li>
<% end %>
</ul>

<% end %>
